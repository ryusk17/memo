{
  "workbench.iconTheme": "vscode-great-icons",
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "solid",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ff4500",
    "terminalCursor.foreground": "#ff4500",
    "list.inactiveSelectionForeground": "#e9ecef",
    "tab.activeBorder": "#228a96"
  },
  "emmet.variables": {
    "lang": "ja"
  },
  "editor.tokenColorCustomizations": {
    "comments": "#ffb468"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "workbench.colorTheme": "Absent Contrast (rainglow)",
  "vsnotes.defaultNotePath": "c:\\Users\\narieda\\Documents\\メモ",
  "files.eol": "\n",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.copyWithSyntaxHighlighting": false,
  "todo-tree.tree.showScanModeButton": true,
  "todo-tree.highlights.defaultHighlight": {
    "icon": "flame",
    "type": "tag",
    "foreground": "#F1F117",
    "background": "#070F17",
    "opacity": 20,
    "iconColour": "#FFD62B"
  },
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "type": "line",
      "background": "#F1D069",
      "opacity": 20,
      "gutterIcon": true
    }
  },
  "todo-tree.tree.grouped": true,
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "FIX"],
  "todo-tree.general.tagGroups": {
    "FIXME": ["FIXME", "FIXIT", "FIX", "TODO", "XXX"]
  },
  "workbench.editor.enablePreview": false,
  "php.validate.executablePath": "C:\\php\\php.exe",
  "phpcs.standard": "C:\\Users\\narieda\\Documents\\settings\\phpcs\\CUSTOM\\ruleset.xml",
  "phpmd.rules": "C:\\study\\開発実習用ディレクトリのベース\\開発実習用ディレクトリのベース\\exercises\\settings\\phpmd\\ruleset.xml",
  "editor.tabSize": 2,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "highlight": "#fff"
      }
    }
  },
  "csscomb.preset": "C:\\Users\\narieda\\Documents\\settings\\csscomb\\.csscomb.json",
  "editor.columnSelection": false,
  "code-eol.color": "#6A655D",
  "editor.renderWhitespace": "all",
  "remote.SSH.remotePlatform": {
    "192.168.12.29": "linux",
    "192.168.0.10": "linux",
    "192.168.0.6": "linux",
    "192.168.12.19": "linux",
    "192.168.0.11": "linux",
    "192.168.0.4": "linux",
    "192.168.12.55": "linux",
    "192.168.0.12": "linux",
    "192.168.0.13": "linux",
    "192.168.12.14": "linux"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
