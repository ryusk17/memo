  User yep-user
  HostName 52.68.211.233
  IdentityFile ~/.ssh/dhq.pem
  ServerAliveInterval 15

### dev batch
Host dhc-dev-batch
  User yep-user
  HostName 172.30.1.175
  IdentityFile ~/.ssh/dhq.pem
  #IdentityFile ~/.ssh/key/dch/dev_ec2_key
  ProxyCommand ssh -W %h:%p dhc-dev-gateway
  ServerAliveInterval 15

