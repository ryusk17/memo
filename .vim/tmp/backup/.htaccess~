Order Allow,Deny
Allow from all

Options FollowSymLinks

<IfModule mod_rewrite.c>
RewriteEngine on
Options FollowSymLinks

RewriteCond %{HTTP:X-Forwarded-Proto} =http [NC]
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

RewriteCond %{REQUEST_URI} !^/favicon.ico
RewriteRule ^$ top/$1 [L]

</IfModule>
